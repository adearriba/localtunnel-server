{
  "author": "Roman Shtylman <shtylman@gmail.com>",
  "name": "localtunnel-server",
  "description": "expose localhost to the world",
  "version": "0.0.8",
  "main": "dist/server.js",
  "bin": "dist/server.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/localtunnel/server.git"
  },
  "dependencies": {
    "debug": "3.1.0",
    "esm": "3.2.25",
    "human-readable-ids": "1.0.4",
    "koa": "2.15.3",
    "koa-router": "12.0.1",
    "localenv": "0.2.2",
    "optimist": "0.6.1",
    "pump": "3.0.0",
    "tldjs": "2.3.1",
    "winston": "3.14.2"
  },
  "devDependencies": {
    "@types/debug": "4.1.12",
    "@types/koa": "2.15.0",
    "@types/koa-router": "7.4.8",
    "@types/node": "22.4.1",
    "@types/optimist": "0.0.33",
    "@types/pump": "1.1.3",
    "@types/supertest": "6.0.2",
    "@types/tldjs": "2.3.4",
    "@types/winston": "2.4.4",
    "mocha": "10.7.0",
    "node-dev": "8.0.0",
    "nodemon": "3.1.4",
    "supertest": "7.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "vitest": "2.0.5",
    "ws": "8.18.0"
  },
  "scripts": {
    "test": "vitest",
    "start": "npm run build && node ./dist/server.js --port 3000 --secure",
    "dev": "nodemon --exec ts-node src/server -- --port 3000 --domain localhost:3000 --max-sockets 50",
    "build": "tsc"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "targets": [
      "latest-linux-x64"
    ],
    "outputPath": "dist"
  }
}